{
  "name": "ljsp-js",
  "version": "0.1.0",
  "description": "A lisp-inspired functional library for javascript programmers",
  "repository": {
    "url": "https://github.com/finleap-connect/ljsp-js"
  },
  "jsnext:main": "dist/index.js",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "release": "npm publish",
    "test": "jest --watch",
    "test:once": "jest",
    "lint": "eslint lib/**/*.ts tests/**/*.js --fix --max-warnings=0",
    "prettify:all": "prettier --write \"**/*.js*\""
  },
  "files": [
    "release"
  ],
  "keywords": [
    "lisp",
    "javascript",
    "functional",
    "util",
    "utils",
    "utilities",
    "functions",
    "toolkit",
    "fp",
    "functional programming",
    "clojure"
  ],
  "author": [
    "Joseph Crick <joseph.crick@finleap.com>",
    "Dania Duany <dania.duany@finleap.com>",
    "Ashish Santikari <hi@ashishsantikari.info>",
    "Josip Brƒçina <josipbrchina@gmail.com>"
  ],
  "license": "ISC",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.5",
    "eslint": "^7.12.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.1.2",
    "rollup": "^2.32.1",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-url": "^3.0.1",
    "rollup-plugin-visualizer": "^4.1.2",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  }
}
